public class Intlist{
	public int first;
	public Intlist rest;
	

	public static void main(String[]args){
		Intlist L = new Intlist();
		L.first = 5;
		L.rest = null;
		/**
		这样的列表很难使用 但是也是可以用的
		用法是 L.first 设置值 L.rest又是一个Intlist
		L.rest.first 是下一个值
		**/

		L.rest = new Intlist();
		L.rest.first = 10;

		L.rest.rest = new Intlist();
		L.rest.rest.first = 15;
		L.rest.rest.rest = null;
	}
}